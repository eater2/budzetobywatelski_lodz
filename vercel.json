{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "functions": {
    "api/streetview.js": {
      "runtime": "edge"
    }
  },
  "rewrites": [
    {
      "source": "/projekty/(.*)",
      "destination": "/projekty/$1.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}